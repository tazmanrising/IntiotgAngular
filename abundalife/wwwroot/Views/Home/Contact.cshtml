                        @*<iframe height="300" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://www.google.com/maps/place/208+3rd+Ave/40.221477,-74.002675,17z/data=!3m1!4b1!4m2!3m1!1s0x89c22641da53feff:0xf284a65232e3a5b9&output=embed"></iframe>*@
                        @*<iframe height="300" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://goo.gl/maps/PE1wm"></iframe>*@
                        @*<iframe width="425" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.ca/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=200+avenue+road+Cambridge+ON&amp;aq=&amp;sll=49.303974,-84.738438&amp;sspn=15.951426,28.256836&amp;ie=UTF8&amp;hq=&amp;hnear=200+Avenue+Rd,+Cambridge,+Ontario+N1R+7Z1&amp;t=m&amp;z=14&amp;ll=43.379803,-80.302328&amp;output=embed"></iframe><br /><small><a href="https://maps.google.ca/maps?f=q&amp;source=embed&amp;hl=en&amp;geocode=&amp;q=200+avenue+road+Cambridge+ON&amp;aq=&amp;sll=49.303974,-84.738438&amp;sspn=15.951426,28.256836&amp;ie=UTF8&amp;hq=&amp;hnear=200+Avenue+Rd,+Cambridge,+Ontario+N1R+7Z1&amp;t=m&amp;z=14&amp;ll=43.379803,-80.302328" style="color:#0000FF;text-align:left">View Larger Map</a></small>*@
                        @*<iframe width="425" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://goo.gl/maps/PE1wm"></iframe><br /><small><a href="https://maps.google.ca/maps?f=q&amp;source=embed&amp;hl=en&amp;geocode=&amp;q=200+avenue+road+Cambridge+ON&amp;aq=&amp;sll=49.303974,-84.738438&amp;sspn=15.951426,28.256836&amp;ie=UTF8&amp;hq=&amp;hnear=200+Avenue+Rd,+Cambridge,+Ontario+N1R+7Z1&amp;t=m&amp;z=14&amp;ll=43.379803,-80.302328" style="color:#0000FF;text-align:left">View Larger Map</a></small>*@
                        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d3046.44185269553!2d-74.002675!3d40.22147699999999!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c22641da53feff%3A0xf284a65232e3a5b9!2s208+3rd+Ave!5e0!3m2!1sen!2sus!4v1401676800417" width="600" height="450" frameborder="0" style="border:0"></iframe>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-sm-6">
                    <div class="cwell">
                        <!-- Contact form -->
                        <h3 class="title">Contact Form</h3>
                        <div class="form">
                            <!-- Contact form (not working)-->
                            <form class="form-horizontal" method="POST">
                                <!-- Name -->
                                <div class="form-group">
                                    <label class="control-label col-md-3" for="name">Name *</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="name" name="name" required>
                                    </div>
                                </div>
                                <!-- Email -->
                                <div class="form-group">
                                    <label class="control-label col-md-3" for="email">Email *</label>
                                    <div class="col-md-9">
                                        <input type="email" class="form-control" id="email" name="email" required>
                                    </div>
                                </div>
                                <!-- Website -->
                                <div class="form-group">
                                    <label class="control-label col-md-3" for="website">Website</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="website" name="website">
                                    </div>
                                </div>
                                <!-- Comment -->
                                <div class="form-group">
                                    <label class="control-label col-md-3" for="comment">Comment</label>
                                    <div class="col-md-9">
                                        <textarea class="form-control" id="comment" name="comment" rows="3"></textarea>
                                    </div>
                                </div>
                                <!-- Buttons -->
                                <div class="form-group">
                                    <!-- Buttons -->
                                    <div class="col-md-9 col-md-offset-3">
                                        <button type="submit" class="btn btn-info">Submit</button>
                                        <button type="reset" class="btn btn-warning">Reset</button>
                                        @if (ViewBag.MailSent != null && ViewBag.MailSent)
                                        {
                                            <script> alert("Mail Sent!"); </script>
                                        }
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cwell">
                        <!-- Address section -->
                        <h3 class="title">Address</h3>

                        <div class="address">
                            <address>
                                <!-- Company name -->
                                <h4>Abunda Life</h4>
                                <!-- Address -->
                                208 3rd Third Avenue<br>
                                Asbury Park, NJ 07712<br>
                                <!-- Phone number -->
                                <abbr title="Phone">P:</abbr> (732) 775-7575.
                            </address>

                            <address>
                                <!-- Name -->
                                <h4>Contact Email</h4>
                                <!-- Email -->
                                <a href="mailto:frontdesk@abundalife.com" target="_top">frontdesk@abundalife.com</a>
                            </address>

                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>
<!-- Content ends -->
